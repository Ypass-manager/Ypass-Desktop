<UserControl x:Class="YpassDesktop.Views.EncryptTestPageView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:YpassDesktop.ViewModels"
             d:DesignHeight="450"
             d:DesignWidth="800"
             x:CompileBindings="True"
             x:DataType="vm:EncryptTestPageViewModel"
             mc:Ignorable="d">
	<Design.DataContext>
		<vm:EncryptTestPageViewModel />
	</Design.DataContext>

	<StackPanel>
		<TextBlock Text="Test concernant le chiffrement"></TextBlock>

		<StackPanel>
			<TextBox Text="Test concernant le chiffrement"></TextBox>

			<TextBox Text="{Binding password}"></TextBox>
			<TextBox Text="{Binding encryptPassword}"></TextBox>
			
			
			

			
			<TextBox Text="{Binding decryptPassword}"></TextBox>
		</StackPanel>
		
		
	</StackPanel>
	</UserControl>