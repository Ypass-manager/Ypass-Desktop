<UserControl x:Class="YpassDesktop.Views.EncryptTestPageView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:YpassDesktop.ViewModels"
             d:DesignHeight="450"
             d:DesignWidth="800"
             x:CompileBindings="True"
             x:DataType="vm:EncryptTestPageViewModel"
             mc:Ignorable="d">
	<Design.DataContext>
		<vm:EncryptTestPageViewModel />
	</Design.DataContext>

	<StackPanel>
		<TextBlock Text="Test concernant le chiffrement"></TextBlock>

		<StackPanel>
			<TextBox Text="Test concernant le chiffrement"></TextBox>

			<!-- Display SALT_CRITICAL -->
			<TextBox Text="{Binding SaltCritical}"></TextBox>

			<!-- Display SALT_CRITICAL_ENCRYPT -->
			<TextBox Text="{Binding SaltCriticalEncrypt}"></TextBox>

			<!-- Display salt_critical_decrypt -->
			<TextBox Text="{Binding SaltCriticalDecrypt}"></TextBox>
		</StackPanel>
		
		
	</StackPanel>
	</UserControl>