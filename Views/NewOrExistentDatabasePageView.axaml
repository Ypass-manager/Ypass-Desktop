<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:YpassDesktop.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="YpassDesktop.Views.NewOrExistentDatabasePageView"
             x:CompileBindings="True"
             x:DataType="vm:NewOrExistentDatabaseViewModel">

	<StackPanel Spacing="10">
		<Button Content="Go back" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Command="{Binding GoBackCommand}"/>
		<Label HorizontalAlignment="Left" VerticalAlignment="Top" Content="Connection" Margin="62,-3,0,0"/>
		<Button Content="Choose a existent account list" HorizontalAlignment="Center" VerticalAlignment="Top" Width="100" Command="{Binding NavigateNextCommand}" CommandParameter="ExistentListCommand"/>
		<Button Content="Create a new account list" HorizontalAlignment="Center" VerticalAlignment="Top" Width="100" Command="{Binding NavigateNextCommand}" CommandParameter="NewListCommand"/>
	</StackPanel>
</UserControl>
