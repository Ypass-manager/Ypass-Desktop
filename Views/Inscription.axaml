<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="YpassDesktop.Views.Inscription"
             Title="Inscription">
  <StackPanel Spacing="10">
	  <Button Content="Return" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="0,0,0,0"/>
        <Label HorizontalAlignment="Left" VerticalAlignment="Top" Content="Sign up" Margin="62,-3,0,0"/>
        <TextBlock>. Your username :</TextBlock>
        <TextBox Text="{Binding username}" />
        <TextBlock>. Your password :</TextBlock>
        <TextBox Text="{Binding password}" />
        <TextBlock>. The name of your account list :</TextBlock>
        <TextBox Text="{Binding databaseName}" />
        <Button Content="Enter" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="35,3,0,0"/>
  </StackPanel>
</UserControl>
